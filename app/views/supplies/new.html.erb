<% provide(:title, 'New Supply') %>

<%= render 'supplies/sidebar' %>

<main class="col-md-9 ms-sm-auto col-lg-10 px-md-4" data-turbo="false">
  <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
    <h1 class="h2">New supply</h1>
    <%= link_to "Back to supplies", supplies_path, class: "btn btn-outline-primary" %>
  </div>
  <%= render "form", supply: @supply %>
</main>
<%= render 'layouts/closing' %>